{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es6", "es2017","dom"]
  },
  "include": ["src"],
  "exclude": [
    "node_modules"
  ]
}
